    "photoswipe": "5.4.4",
    "photoswipe-dynamic-caption-plugin": "1.2.7",
    "pinia": "2.3.1",
    "v3-infinite-loading": "1.3.2",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue-spinner": "1.0.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.5",
    "@types/jsdom": "21.1.7",
