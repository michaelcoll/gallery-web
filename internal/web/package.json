    "photoswipe": "^5.3.8",
    "photoswipe-dynamic-caption-plugin": "^1.2.7",
    "pinia": "^2.1.4",
    "v3-infinite-loading": "^1.3.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-spinner": "^1.0.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
