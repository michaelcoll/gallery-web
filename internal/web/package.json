    "photoswipe-dynamic-caption-plugin": "^1.2.7",
    "pinia": "^2.2.2",
    "v3-infinite-loading": "^1.3.2",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3",
    "vue-spinner": "^1.0.4"
