    "dayjs": "^1.11.6",
    "photoswipe": "^5.3.3",
    "photoswipe-dynamic-caption-plugin": "^1.2.7",
    "pinia": "^2.0.24",
    "v3-infinite-loading": "^1.2.2",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-spinner": "^1.0.4"
